{
  "manifest_version": 2,
  "name": "Google_Meet_Autologger",
  "version": "6.9",
  "description":"logs you out of the meet if less than 10 people present",
  "icons":{
    "128":"icon128.png",
    "48":"icon48.png",
    "16":"icon16.png"
  },
  "browser_action":{
    "default_icon":"icon16.png",
    "default_title":"vatsal sucks"


  },
  "content_scripts":[
    {
    "matches":[
      "https://meet.google.com/*"
    ],
    "js":["content.js"]

  }
],
  "background":{
    "scripts":["back.js"]
  }



 }
